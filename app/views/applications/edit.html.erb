

<!-- This page is a form that is used when a supervisor chooses to review an application -->

<%= form_for(@app) do |f| %>

	<%= f.label :Specify_Change %>
	<%= f.text_area :reqChange %>
	<%= f.submit "Approve Application" %>
	<%= f.submit "Refuse Application" %>
	<%= f.submit "Request Changes" %>
<%end %>